    var i = 0;

    if (i == 0) {
        i = 1;
            var elemm = document.getElementById("kg");
        
        
        var width = 1;
        var id = setInterval(frame , 500);
        function frame() {
            if (width >= 80) {
                
                clearInterval(id);
                i = 0;
            } else {
                width++;
             
                elemm.style.width = width + "%";

            
            }
        }
    }

    </script>

